[Unit]
Description=Vanna API Service
After=network.target

[Service]
User=ubuntu
WorkingDirectory=/home/ubuntu/Vanna
Environment="PATH=/home/ubuntu/Vanna/venv/bin"
EnvironmentFile=/home/ubuntu/Vanna/.env
ExecStart=/home/ubuntu/Vanna/venv/bin/gunicorn -w 4 -k uvicorn.workers.UvicornWorker app:server.app --bind 0.0.0.0:8081
Restart=always

[Install]
WantedBy=multi-user.target